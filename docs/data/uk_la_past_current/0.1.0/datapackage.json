            "role": "author"
        }
    ],
    "custom": {
        "build": "uk_local_authority_names_and_codes.build:create_all_files",
        "tests": [
            "test_basic_requirements",
            "test_time",
            "test_values",
            "test_current"
        ],
        "download_options": {
            "gate": "default",
            "survey": "default",
            "header_text": "default"
